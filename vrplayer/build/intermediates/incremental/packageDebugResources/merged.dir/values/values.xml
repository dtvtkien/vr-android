<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:ns1="http://schemas.android.com/tools">
    <color name="bg_media_controller">#64000000</color>
    <color name="black">#FF000000</color>
    <color name="black_30">#4D000000</color>
    <color name="colorAccent">#64000000</color>
    <color name="colorPrimary">#8a8a8a</color>
    <color name="colorPrimaryDark">#8a8a8a</color>
    <color name="purple_200">#FFBB86FC</color>
    <color name="purple_500">#FF6200EE</color>
    <color name="purple_700">#FF3700B3</color>
    <color name="teal_200">#FF03DAC5</color>
    <color name="teal_700">#FF018786</color>
    <color name="white">#FFFFFFFF</color>
    <dimen name="large_spacing">48dp</dimen>
    <dimen name="media_control_height">50dp</dimen>
    <dimen name="medium_spacing">16dp</dimen>
    <dimen name="small_spacing">8dp</dimen>
    <dimen name="tiny_spacing">4dp</dimen>
    <dimen name="x_medium_spacing">24dp</dimen>
    <dimen name="xx_medium_spacing">32dp</dimen>
    <item name="statusbarutil_fake_status_bar_view" type="id"/>
    <item name="statusbarutil_translucent_view" type="id"/>
    <string name="control_title">Play/Pause</string>
    <string name="invalid_progressive_playback">Invalid progressive playback</string>
    <string name="ok">OK</string>
    <string name="simple_but_powerful">Simple But Powerful</string>
    <string name="unknown">Unknown</string>
    <string name="vr_player_title">VRPlayer</string>
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="MediaControllerTextTheme">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="SeekBarTheme" parent="Widget.AppCompat.SeekBar">
        <item name="android:progressBackgroundTint">@color/colorPrimaryDark</item>
        <item name="android:progressTint">@color/white</item>
        <item name="android:colorControlActivated">@color/white</item>
        <item name="android:thumbTint">@color/white</item>
    </style>
    <style name="Theme.DemoVr180" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        
        <item name="android:statusBarColor" ns1:targetApi="l">?attr/colorPrimaryVariant</item>
        
    </style>
    <declare-styleable name="CircleProgressBar">
        <attr format="integer" name="min"/>
        <attr format="integer" name="max"/>
        <attr format="integer" name="progress"/>
        <attr format="color" name="progressbarColor"/>
        <attr format="dimension" name="progressBarThickness"/>
        <attr format="integer" name="animatedDurationInSeconds"/>
    </declare-styleable>
</resources>